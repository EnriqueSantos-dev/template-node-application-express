{
  "name": "template-node-express",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:dev": "tsx watch ./src/server.ts",
    "start": "node ./dist/server.js",
    "build": "tsc",
    "db:dev:stop": "docker compose rm -svf",
    "db:dev:start": "docker compose up -d --remove-orphans",
    "db:dev:restart": "pnpm db:dev:stop && pnpm db:dev:start && sleep 1 && pnpm prisma:dev",
    "prisma:dev": "dotenv prisma migrate dev",
    "prisma:studio:dev": " prisma studio",
    "prisma:dev:reset": "prisma migrate reset",
    "db:test:stop": "docker compose rm -svf",
    "db:test:start": "docker compose -f ./docker-compose.test.yml up -d --remove-orphans",
    "db:test:restart": "pnpm db:test:stop && pnpm db:test:start && sleep 1 && pnpm prisma:test",
    "prisma:test": "dotenv -e .env.testing prisma migrate deploy",
    "prisma:studio:test": "dotenv -e .env.testing prisma studio",
    "prisma:test:reset": "prisma migrate reset",
    "prisma:test:studio": "dotenv -e .env.testing prisma studio",
    "lint": "pnpm eslint src/**/*.ts",
    "fix": "pnpm lint --fix",
    "prepare": "husky install",
    "staged": "pnpm lint-staged",
    "cz": "git-cz",
    "test": "jest --passWithNoTests",
    "test:watch": "pnpm run test --watch",
    "test:cov": "jest --coverage",
    "test:int": "dotenv -e .env.testing -- jest -i --watch --no-cache --config=jest-int.config.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
	"config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@prisma/client": "^4.9.0",
    "bcrypt": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.32",
    "@swc/jest": "^0.2.24",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.11.18",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "commitizen": "^4.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "dotenv-cli": "^7.0.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.3",
    "prisma": "^4.9.0",
    "jest": "^29.4.1",
    "ts-jest": "^29.0.5",
    "tsx": "^3.12.2",
    "typescript": "^4.9.5"
  }
}
